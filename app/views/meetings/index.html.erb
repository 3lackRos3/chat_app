<div class="col-md-6 col-md-offset-3">
  <h1>Listing meetings</h1>

<table>
  <thead>
    <tr>
      <th>Caller</th>
      <th>Receiver</th>
      <th>Order</th>
      <th>Call type</th>
      <th>Duration</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.caller.name %></td>
        <td><%= meeting.receiver.name %></td>
        <td><%= meeting.order_id %></td>
        <td><%= meeting.call_type %></td>
        <td><%= meeting.duration %></td>
        <td><%= link_to 'Show', meeting %></td>
        <td><%= link_to 'Edit', edit_meeting_path(meeting) %></td>
        <td><%= link_to 'Destroy', meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting', new_meeting_path %>
</div>
